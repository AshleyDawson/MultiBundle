{
  "name": "ashleydawson/multibundle",
  "type": "library",
  "description": "Register multiple bundles within the Symfony2 kernel",
  "license": "MIT",
  "keywords": [
    "symfony",
    "symfony2",
    "multiple",
    "bundle",
    "dependencies"
  ],
  "authors": [
    {
      "name": "Ashley Dawson",
      "email": "ashley@ashleydawson.co.uk"
    }
  ],
  "require": {
    "php": ">=5.3",
    "symfony/symfony": "~2.3|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.4"
  },
  "autoload": {
    "psr-4": { "AshleyDawson\\MultiBundle\\": "lib" }
  },
  "config": {
    "bin-dir": "bin"
  }
}
